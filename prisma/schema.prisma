generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id              String    @id @default(uuid())
  username        String
  password        String
  email           String    @unique
  data            Json
  createDate      DateTime  @default(now())
  lastLoginTime   DateTime?
}
